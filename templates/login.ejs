<%- include _header.ejs %>
<h1>Login</h1>
<form action="/user/authenticate" method="POST">
  <div class="form-row">
    <label for=username>Username / Email</label>
    <input type=text name=username />
  </div>
  <div class="form-row">
    <label for=password>Password</label>
    <input type=password name=password />
  </div>
  <button>Submit</button>
</form>
